            <geom type="mesh" rgba="0.792157 0.819608 0.933333 1" mesh="right_wheel_link" />
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <actuator>
    <position name="left_hip" joint="left_hip" ctrllimited="true" kp="5" ctrlrange="-8 8" gear="1" />
    <position name="left_knee" joint="left_knee" ctrllimited="true" kp="5"  ctrlrange="-4 4" gear="1" />
    <velocity name="left_wheel" joint="left_wheel" ctrllimited="true"   ctrlrange="-2 2" gear="1" />
    <position name="right_hip" joint="right_hip" ctrllimited="true" kp="5"  ctrlrange="-8 8" gear="1" />
    <position name="right_knee" joint="right_knee" ctrllimited="true" kp="5"  ctrlrange="-4 4" gear="1" />
    <velocity name="right_wheel" joint="right_wheel" ctrllimited="true" ctrlrange="-2 2" gear="1" />
  </actuator>

  <sensor>
    <actuatorpos name="left_hip_p" actuator="left_hip" />
    <actuatorvel name="left_hip_v" actuator="left_hip" />
    <actuatorfrc name="left_hip_f" actuator="left_hip" noise="0.001" />
    <actuatorpos name="left_knee_p" actuator="left_knee" />
    <actuatorvel name="left_knee_v" actuator="left_knee" />
    <actuatorfrc name="left_knee_f" actuator="left_knee" noise="0.001" />
    <actuatorpos name="left_wheel_p" actuator="left_wheel" />
    <actuatorvel name="left_wheel_v" actuator="left_wheel" />
    <actuatorfrc name="left_wheel_f" actuator="left_wheel" noise="0.001" />
    <actuatorpos name="right_hip_p" actuator="right_hip" />
    <actuatorvel name="right_hip_v" actuator="right_hip" />
    <actuatorfrc name="right_hip_f" actuator="right_hip" noise="0.001" />
    <actuatorpos name="right_knee_p" actuator="right_knee" />
    <actuatorvel name="right_knee_v" actuator="right_knee" />
    <actuatorfrc name="right_knee_f" actuator="right_knee" noise="0.001" />
    <actuatorpos name="right_wheel_p" actuator="right_wheel" />
    <actuatorvel name="right_wheel_v" actuator="right_wheel" />
    <actuatorfrc name="right_wheel_f" actuator="right_wheel" noise="0.001" />
    
    <framequat name="orientation" objtype="site" noise="0.001" objname="imu" />
    <gyro name="angular_velocity" site="imu"  noise="0.005" cutoff="34.9"/>
    <velocimeter name="linear_velocity" site="imu" noise="0.005" cutoff="34.9"/>
  </sensor>
</mujoco>