        <joint name="left_hip" pos="0 0 0" axis="-1 0 0" range="-1.57 1.57" actuatorfrcrange="-8 8" damping="0.56"/>
        <geom type="mesh" rgba="1 1 1 1" mesh="left_hip_link" contype="1" conaffinity="0" density="0" group="1" class="visualgeom" />
        <geom type="mesh" rgba="1 1 1 1" mesh="left_hip_link" />
        <body name="left_knee_link" pos="0.011886 -0.0043428 0.19995" quat="0.99984 0.017892 0 0">
          <inertial pos="0.01 0.00405816 0.0780025" quat="0.999683 -0.0251816 0 0" mass="0.235606" diaginertia="0.00125058 0.00120796 5.83331e-05" />
          <joint name="left_knee" pos="0 0 0" axis="-1 0 0" range="-2.51 2.51" actuatorfrcrange="-4 4" damping="0.56" />
