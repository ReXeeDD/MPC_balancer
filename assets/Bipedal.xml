  </default>

  <asset>
    <mesh name="base_link" content_type="model/stl" file="base_link.STL" />
    <mesh name="left_hip_link" content_type="model/stl" file="left_hip_link.STL" />
    <mesh name="left_knee_link" content_type="model/stl" file="left_knee_link.STL" />
    <mesh name="right_hip_link" content_type="model/stl" file="right_hip_link.STL" />
