          <geom type="mesh" rgba="1 1 1 1" mesh="left_knee_link" />
          <body name="left_wheel_link" pos="0 0.0099672 0.19975" quat="0.849056 0.528302 0 0">
            <inertial pos="0.0398848 0 0" mass="0.511294" diaginertia="0.00206123 0.00104922 0.00104922" />
