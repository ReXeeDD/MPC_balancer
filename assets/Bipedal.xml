          <body name="right_wheel_link" pos="0 -0.0095996 0.19977" quat="-3.58152e-06 8.15558e-07 -0.222029 0.97504">
            <inertial pos="0.0398848 0 0" mass="0.511294" diaginertia="0.00206123 0.00104922 0.00104922" />
            <joint name="right_wheel" pos="0 0 0" axis="-1 0 0" actuatorfrcrange="-2 2" limited="true" range="-1000000 1000000" damping="0.12" />
