    <camera name="fixed" pos="0 -3.0 0.9730745173996977" xyaxes="1 0 0 0 0 1" />
    <camera name="track" mode="trackcom" pos="0 -3.0 0.9730745173996977" xyaxes="1 0 0 0 0 1" />
    <body name="root" pos="0 0 0.547307451739969775" quat="1 0 0 0">
      <freejoint name="root" />
      <inertial  pos="0.01 -0.0196003228135452 -0.0227358081145863" mass="2.58680345236989" fullinertia="0.0215162702470085 0.02233115289391 0.0214719602558426 -5.17133997903683e-06 -4.56150022793563e-06 2.30329361555432e-05"/>
      <site name="imu" size="0.01" pos="0 0 0" />
      <geom type="mesh" rgba="1 1 1 1" mesh="base_link" contype="1" conaffinity="0" density="0" group="1" class="visualgeom" />
      <geom type="mesh" rgba="1 1 1 1" mesh="base_link" />
      <body name="left_hip_link" pos="0.00888 0.07579 -0.053421" quat="0.00385111 0.707095 0.707098 0.00385112">
