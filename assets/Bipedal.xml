        <inertial pos="0.0221476 5.51885e-08 0.0832579" quat="1 7.78778e-07 0.000319768 6.4318e-06" mass="0.548021" diaginertia="0.00300532 0.00292396 0.000248069" />
        <joint name="right_hip" pos="0 0 0" axis="1 0 0" range="-1.57 1.57" actuatorfrcrange="-8 8" damping="0.56"/>
